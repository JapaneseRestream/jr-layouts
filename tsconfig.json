{
	"compilerOptions": {
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUncheckedIndexedAccess": true,
		"noPropertyAccessFromIndexSignature": true,
		"skipLibCheck": true,
		"isolatedModules": true,

		"importHelpers": true,
		"sourceMap": true,

		"module": "ES2022",
		"moduleResolution": "bundler",
		"target": "ES2022",
		"lib": ["ES2022", "DOM"],
		"types": []
	}
}
