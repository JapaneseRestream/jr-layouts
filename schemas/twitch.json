{
	"$schema": "http://json-schema.org/draft-04/schema",

	"type": "object",
	"additionalProperties": false,
	"required": ["channelInfo"],
	"properties": {
		"channelInfo": {
			"type": "object",
			"additionalProperties": false,
			"required": ["target", "ours"],
			"properties": {
				"target": {
					"type": "object",
					"additionalProperties": false,
					"required": ["title", "game", "logo"],
					"default": {"title": "", "game": "", "logo": ""},
					"properties": {
						"title": {
							"type": "string"
						},
						"game": {
							"type": "string"
						},
						"logo": {
							"type": "string"
						}
					}
				},
				"ours": {
					"type": "object",
					"additionalProperties": false,
					"required": ["title", "game", "logo"],
					"default": {"title": "", "game": "", "logo": ""},
					"properties": {
						"title": {
							"type": "string"
						},
						"game": {
							"type": "string"
						},
						"logo": {
							"type": "string"
						}
					}
				}
			}
		}
	}
}
