{
	"name": "jr-layouts",
	"version": "0.0.0",
	"private": true,
	"browserslist": [
		"last 2 chrome versions"
	],
	"scripts": {
		"clean": "del-cli dashboard graphics",
		"prod-build": "cross-env NODE_ENV=production yarn build",
		"build": "yarn clean && run-p \"build:*\"",
		"build:extension": "tsc -p tsconfig.extension.json",
		"build:dashboard": "webpack --config-name dashboard",
		"build:graphics": "webpack --config-name graphics",
		"watch": "yarn clean && run-p \"watch:*\"",
		"watch:extension": "yarn build:extension -w --preserveWatchOutput",
		"watch:dashboard": "yarn build:dashboard -w",
		"watch:graphics": "yarn build:graphics -w",
		"generate-schema": "ts-node scripts/generate-schema.ts",
		"genschema": "run-p \"genschema:*\""
	},
	"dependencies": {
		"axios": "^0.18.0",
		"date-fns": "^2.0.0-alpha.27",
		"googleapis": "^39.2.0",
		"joi": "^14.3.1",
		"lodash": "^4.17.11",
		"moment": "^2.24.0"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"@material-ui/core": "^3.9.3",
		"@material-ui/icons": "^3.0.2",
		"@types/clean-webpack-plugin": "^0.1.3",
		"@types/html-webpack-plugin": "^3.2.0",
		"@types/joi": "^14.0.1",
		"@types/lodash": "^4.14.123",
		"@types/mini-css-extract-plugin": "^0.2.0",
		"@types/node": "10",
		"@types/react": "^16.4.18",
		"@types/react-dom": "^16.0.11",
		"@types/styled-components": "4.1.8",
		"@types/webpack": "^4.4.27",
		"@types/webpack-bundle-analyzer": "^2.13.1",
		"@types/webpack-merge": "^4.1.4",
		"@types/webpack-node-externals": "^1.6.3",
		"babel-loader": "^8.0.5",
		"clean-webpack-plugin": "^2.0.1",
		"concurrently": "^4.1.0",
		"cross-env": "^5.2.0",
		"css-loader": "^2.1.1",
		"del": "^4.1.0",
		"del-cli": "^1.1.0",
		"file-loader": "^3.0.1",
		"globby": "^9.2.0",
		"html-webpack-plugin": "^4.0.0-beta.5",
		"mini-css-extract-plugin": "^0.6.0",
		"npm-run-all": "^4.1.5",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"styled-components": "^4.2.0",
		"ts-loader": "^5.3.3",
		"ts-node": "^8.1.0",
		"tslib": "^1.9.3",
		"typescript": "3.4.4",
		"typescript-json-schema": "^0.37.0",
		"url-loader": "^1.1.2",
		"use-nodecg": "^0.4.1-1",
		"webpack": "^4.30.0",
		"webpack-bundle-analyzer": "^3.3.2",
		"webpack-cli": "^3.3.0",
		"webpack-merge": "^4.2.1",
		"webpack-node-externals": "^1.7.2",
		"write-json-file": "^3.2.0"
	},
	"nodecg": {
		"compatibleRange": "^1.0.0",
		"dashboardPanels": [
			{
				"name": "current-game",
				"title": "現在のゲーム",
				"width": 3,
				"file": "current-game.html",
				"workspace": "配信管理"
			},
			{
				"name": "twitch-info",
				"title": "Twitch",
				"width": 3,
				"file": "twitch-info.html",
				"workspace": "配信管理"
			},
			{
				"name": "games-list",
				"title": "ゲームリスト",
				"width": 8,
				"file": "games-list.html",
				"workspace": "配信管理"
			},
			{
				"name": "misc-controls",
				"title": "その他操作",
				"width": 3,
				"file": "misc-controls.html",
				"workspace": "配信管理"
			},
			{
				"name": "edit-modal",
				"title": "編集",
				"width": 4,
				"file": "edit-modal.html",
				"dialog": true,
				"dialogButtons": [
					{
						"name": "save",
						"type": "confirm"
					},
					{
						"name": "cancel",
						"type": "dismiss"
					}
				]
			}
		],
		"graphics": [
			{
				"file": "main.html",
				"width": 1920,
				"height": 1080
			}
		]
	}
}
