{
	"name": "jr-layouts",
	"private": true,
	"browserslist": [
		"last 2 chrome versions"
	],
	"scripts": {
		"build": "run-p \"build:*\"",
		"build:extension": "webpack --config-name extension",
		"build:dashboard": "webpack --config-name dashboard",
		"build:graphics": "webpack --config-name graphics",
		"genschema": "run-p \"genschema:*\"",
		"genschema:bundle-config": "del-cli configschema.json && typescript-json-schema --noExtraProps --required --strictNullChecks --out configschema.json src/bundle-config.ts BundleConfig",
